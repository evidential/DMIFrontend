{
    "version": "1.0.0",
    "name": "DMI Tablet",
    "app-name": "DMI Tablet",
    "package-name": "dmitabletmanager",
    "project-type": "webview",
    "project-name": "DMI Tablet",
    "screenOrientation": "landscape",
    "theme": {
        "fullScreen": false,
        "colorAccent": "#e3e3e3",
        "colorPrimary": "#000",
        "colorPrimaryDark": "#000000"
    },
    "icon": "./assets/icon/icon.png",
    "dist-path": "./dist",
    "permission": [
        "android.permission.INTERNET"
    ],
    "description": "",
    "main": "src/main.js",
    "scripts": {
        "start:node": "node .",
