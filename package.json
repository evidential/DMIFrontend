    "dependencies": {
        "@ionic/core": "^6.0.1",
        "@stencil/core": "^2.7.0",
        "@stencil/store": "^1.3.0",
        "androidjs": "^2.0.6",
        "androidjs-builder": "^2.3.2",
        "body-parser": "^1.20.1",
        "express": "^4.18.2",
        "rollup-plugin-dotenv": "^0.3.0",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "socket.io": "^4.6.0",
        "three": "^0.149.0",
        "tween.js": "^16.6.0"
    },
    "devDependencies": {
        "copyfiles": "^2.4.1",
        "del-cli": "^5.0.0"
